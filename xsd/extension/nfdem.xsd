<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
    ct:conformanceTargets="http://reference.niem.gov/niem/specification/naming-and-design-rules/4.0/#ExtensionSchemaDocument"
    targetNamespace="http://www.dtechspace.com/xml-schema/nfdem/0.5/" version="0.5"
    xmlns:nfdem="http://www.dtechspace.com/xml-schema/nfdem/0.5/"
    xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/"
    xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/"
    xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:cbrn="http://release.niem.gov/niem/domains/cbrn/4.0/"
    xmlns:jxdm="http://release.niem.gov/niem/domains/jxdm/6.0/"
    xmlns:mmucc="http://release.niem.gov/niem/codes/mmucc/4.0/"
    xmlns:unece="http://release.niem.gov/niem/codes/unece_rec20/4.0/"
    xmlns:biom="http://release.niem.gov/niem/domains/biometrics/4.0/"
    xmlns:hs="http://release.niem.gov/niem/domains/humanServices/4.0/"
    elementFormDefault="qualified">

    <xs:annotation>
        <xs:documentation> This extension schema defines the elements and types for the Nuclear Forensics Data Exchange Model (NFDEM). </xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../niem/niem-core/4.0/niem-core.xsd" namespace="http://release.niem.gov/niem/niem-core/4.0/"/>
    <xs:import schemaLocation="../niem/proxy/xsd/4.0/xs.xsd" namespace="http://release.niem.gov/niem/proxy/xsd/4.0/"/>
    <xs:import schemaLocation="../niem/domains/cbrn/4.0/cbrn.xsd" namespace="http://release.niem.gov/niem/domains/cbrn/4.0/"/>
    <xs:import schemaLocation="../niem/domains/jxdm/6.0/jxdm.xsd" namespace="http://release.niem.gov/niem/domains/jxdm/6.0/"/>
    <xs:import schemaLocation="../niem/codes/mmucc/4.0/mmucc.xsd" namespace="http://release.niem.gov/niem/codes/mmucc/4.0/"/>
    <xs:import schemaLocation="../niem/codes/unece_rec20/4.0/unece_rec20-misc.xsd" namespace="http://release.niem.gov/niem/codes/unece_rec20/4.0/"/>
    <xs:import schemaLocation="../niem/domains/biometrics/4.0/biom.xsd" namespace="http://release.niem.gov/niem/domains/biometrics/4.0/"/>
    <xs:import schemaLocation="../niem/domains/humanServices/4.0/hs.xsd" namespace="http://release.niem.gov/niem/domains/humanServices/4.0/"/>
    <!--xs:import schemaLocation="../niem/utility/structures/4.0/structures.xsd" namespace="http://release.niem.gov/niem/structures/4.0/"/-->

    <xs:element name="CaseList" type="nfdem:Case">
        <xs:annotation>
            <xs:documentation> CaseList document corresponds to the Case Identifier Form for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:complexType name="Case">
        <xs:annotation>
            <xs:documentation> XML Schema type for Case type which consists of the unique Case Identifier and details of the Case for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:IncidentManager" minOccurs="1"/>
            <xs:element ref="nfdem:DateTimeCreated" minOccurs="1"/>
            <xs:element ref="nfdem:DateTimeUpdated" minOccurs="1"/>
            <xs:element ref="nfdem:CaseStatus" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="1"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element ref="nfdem:CaseCommentList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="AdminWorksheet" type="nfdem:AdminWorksheetType">
        <xs:annotation>
            <xs:documentation> AdminWorksheet document corresponds to the Administrative Worksheet
                form for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="AdminWorksheetType">
        <xs:annotation>
            <xs:documentation> XML Schema type for AdminWorksheet element which corresponds to the
                Administrative Worksheet form for crime scene investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation"/>
            <xs:element ref="nfdem:ArrivalDateTime" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:LightingCondition" minOccurs="1"/>
            <xs:element ref="nfdem:PersonPresentOnArrival" minOccurs="0"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element ref="nfdem:AssistantPreparer" minOccurs="1"/>
            <xs:element ref="nfdem:WeatherCondition" minOccurs="1"/>
            <xs:element name="SceneConsistsOf" type="nc:TextType"/>
            <xs:element name="PersonnelInvolvedAndTheirDuties" type="nc:TextType"/>
            <xs:element name="PreliminarySurveyEvaluation" type="nc:TextType"/>
            <xs:element name="SpecialConditions" type="nc:TextType"/>
            <xs:element name="CrimeSceneConference" type="nc:TextType"/>
            <xs:element name="FinalSurvey" type="nc:TextType"/>
            <xs:element ref="nfdem:CrimeSceneReleasedByList" minOccurs="1"/>
            <xs:element ref="nfdem:CrimeSceneReleasedToList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="RadiationSurveyInfo" type="nfdem:RadiationSurveyInfoType">
        <xs:annotation>
            <xs:documentation> RadiationSurveyInfo document corresponds to the Radiation Survey
                Information form for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="RadiationSurveyInfoType">
        <xs:annotation>
            <xs:documentation> XML Schema type for RadiationSurveyInfo element which corresponds to
                the Radiation Survey Information form for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="cbrn:RadInstrumentInformation" minOccurs="1"/>
            <xs:element ref="nfdem:InstrumentCalibrationDateTime" minOccurs="1"/>
            <xs:element ref="nfdem:RadiationSurveyDoseRateList" minOccurs="1"/>
            <xs:element ref="nfdem:SurveyMap" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="PersonalRadMonitoringLog" type="nfdem:PersonalRadMonitoringLogType">
        <xs:annotation>
            <xs:documentation> PersonalRadMonitoringLog document corresponds to the Personal
                External Radiation Monitoring Log for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="PersonalRadMonitoringLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for PersonalRadMonitoringLog element which
                corresponds to the Personal External Radiation Monitoring Log for crime scene
                investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:PersonMonitored" minOccurs="0"/>
            <xs:element ref="nfdem:PersonMonitoredIdentification"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:PersonalDosimeterReadingList" minOccurs="1"/>
            <xs:element ref="nfdem:GammaDoseRateMeterReadingList" minOccurs="1"/>
            <xs:element ref="nfdem:ThermoluminescentDosimeterReadingList" minOccurs="1"/>                        
        </xs:sequence>
    </xs:complexType>
  
    <xs:element name="SceneInfo" type="nfdem:SceneInfoType">
        <xs:annotation>
            <xs:documentation> SceneInfo element corresponds to the Scence Information or Diagram form for crime scene investigation. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="SceneInfoType">
        <xs:annotation>
            <xs:documentation> XML Schema type for SceneInfo element which corresponds to the Scence Information or Diagram form for crime scene investigation. 
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:DateTimeCreated" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element name="ScaleOrDisclaimer" type="nc:TextType"/>
            <xs:element name="Evidence" type="nc:TextType"/>
            <xs:element name="FixedObjects" type="nc:TextType"/>
            <xs:element name="CompassOrientation" type="nc:TextType"/>
            <xs:element name="KeyLegend" type="nc:TextType"/>
            <xs:element name="SceneDiagram" type="cbrn:ImageFileType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="GeneralChainOfCustody" type="nfdem:GeneralChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> GeneralChainOfCustody document corresponds to the generic Chain of
                Custody form for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="GeneralChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> XML Schema type for GeneralChainOfCustody element which corresponds
                to the generic Chain of Custody form for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:SampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="FieldEvidenceChainOfCustody" type="nfdem:FieldEvidenceChainofCustodyType">
        <xs:annotation>
            <xs:documentation> FieldEvidenceChainOfCustody document corresponds to the Chain of Custody form for the field evidence collected as part of the crime scene investigation. 
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:complexType name="FieldEvidenceChainofCustodyType">
        <xs:annotation>
            <xs:documentation> XML Schema type for FieldEvidenceChainOfCustody element which corresponds to the Chain of Custody form for the field evidence collected as part of the crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="nfdem:FieldEvidenceList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="EnvironmentalChainOfCustody" type="nfdem:EnvironmentalChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> EnvironmentalChainOfCustodyType document corresponds to the Chain of
                Custody form for Environmental Analysis. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="EnvironmentalChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> XML Schema type for EnvironmentalChainOfCustody element which
                corresponds to the Chain of Custody form for Environmental Analysis.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:EnvironmentalSampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="GeoChemicaChainOfCustody" type="nfdem:GeoChemicaChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> GeoChemicaChainOfCustodyType document corresponds to the Chain of
                Custody form for GeoChemical Analysis. </xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:complexType name="GeoChemicaChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> XML Schema type for GeoChemicaChainOfCustodyType element which
                corresponds to the Chain of Custody form for GeoChemical Analysis.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:GeoChemicalSampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="RadiologicalChainOfCustody" type="nfdem:RadiologicalChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> RadiologicalChainOfCustodyType document corresponds to the Chain of
                Custody form for Radiological Analysis. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="RadiologicalChainOfCustodyType">
        <xs:annotation>
            <xs:documentation> XML Schema type for RadiologicalChainOfCustody element which corresponds
                to the Chain of Custody form for Radiological Analysis. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:RadiologicalSampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="PhotographicLog" type="nfdem:PhotographicLogType">
        <xs:annotation>
            <xs:documentation> PhotographicLog document corresponds to the Photographic Log for
                crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="PhotographicLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for PhotographicLog element which corresponds to the
                Photographic Log for crime scene investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element name="Date" type="niem-xs:date"/>                
            <xs:element name="Photographer" type="nc:PersonNameTextType"/>  
            <xs:element ref="nfdem:Comment" minOccurs="0"/>        
            <xs:element name="PhotoList" minOccurs="1"/>        
        </xs:sequence>
    </xs:complexType>

    <xs:element name="EvidenceRecoveryLogCover" type="nfdem:EvidenceRecoveryLogCoverType">
        <xs:annotation>
            <xs:documentation> EvidenceRecoveryLogCover document corresponds to the Evidence
                Recovery Log Cover Sheet for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="EvidenceRecoveryLogCoverType">
        <xs:annotation>
            <xs:documentation> XML Schema type for EvidenceRecoveryLogCover element which
                corresponds to the Evidence Recovery Log Cover Sheet for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:DateTimeSampleCollected" minOccurs="1"/>
            <xs:element ref="nfdem:Comment" minOccurs="1"/>
            <xs:element name="SiteNumber" type="niem-xs:string"/>
            <xs:element name="TeamNumber" type="niem-xs:string"/>
            <xs:element name="GridNumber" type="niem-xs:string"/>
            <xs:element ref="nfdem:SearchTeamPersonnelList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="EvidenceRecoveryLog" type="nfdem:EvidenceRecoveryLogType">
        <xs:annotation>
            <xs:documentation> EvidenceRecoveryLog document corresponds to the Evidence Recovery Log
                for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="EvidenceRecoveryLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for EvidenceRecoveryLog element which corresponds to
                the Evidence Recovery Log for crime scene investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element ref="nfdem:PersonnelList"/>
            <xs:element ref="nfdem:EvidenceRecoveryItemList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="CrimeSceneEntryLog" type="nfdem:CrimeSceneEntryLogType">
        <xs:annotation>
            <xs:documentation> CrimeSceneEntryLog document corresponds to the Crime Scene Entry Log for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="CrimeSceneEntryLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for CrimeSceneEntryLog element which corresponds to
                the Crime Scene Entry Log for crime scene investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:Agency" minOccurs="1"/>
            <xs:element ref="nfdem:PersonEntryList"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:DateTimeUpdated"/> 
        </xs:sequence>
    </xs:complexType>

    <xs:element name="RadioactiveMaterialLabel" type="nfdem:RadioactiveMaterialLabelType">
        <xs:annotation>
            <xs:documentation> RadioactiveMaterialLabel document corresponds to the Radioactive
                Material Label for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="RadioactiveMaterialLabelType">
        <xs:annotation>
            <xs:documentation> XML Schema type for RadioactiveMaterialLabel element which
                corresponds to the Radioactive Material Label for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:Description" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:DateTimeSampleCollected" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:MaterialSuspected" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SurveyDateTime" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:Surveyor" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SurveyOrganization" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SurveyUnit" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SwipeTestResult" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="EnvironmentalReport" type="nfdem:EnvironmentalReportType">
        <xs:annotation>
            <xs:documentation> EnvironmentalReport document corresponds to the Environmental Lab
                Report for lab analysis and results. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="EnvironmentalReportType">
        <xs:annotation>
            <xs:documentation> XML Schema type for EnvironmentalReport element which corresponds to
                the the Environmental Lab Report for lab analysis and results. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:AirFilterMediaType" minOccurs="0"/>
            <xs:element ref="nfdem:AirFilterSize" minOccurs="0"/>
            <xs:element ref="nfdem:AirFilterUnit" minOccurs="0"/>
            <xs:element ref="nfdem:Analyst" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:AnalysisMethod" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:AnalysisDateTime" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:DilutionFactor" minOccurs="0"/>
            <xs:element ref="nfdem:MethodDetectionLimit" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:AnalyteList"/>
            <xs:element ref="nfdem:DateTimeSampleCollected" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:DateTimeSamplePrepared" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:LabSampleID" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="cbrn:LowerLimitValue" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:MoisutrePercentage" minOccurs="0"/>
