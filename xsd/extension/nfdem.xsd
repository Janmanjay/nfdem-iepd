    <xs:element name="SampleInformation" type="nfdem:SampleInformationType">
        <xs:annotation>
            <xs:documentation> SampleInfo document corresponds to the Sample Information form for
                lab analysis and results. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="SampleInformationType">
        <xs:annotation>
            <xs:documentation> XML Schema type for SampleInfo element which corresponds to the the
