        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:Description" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:Comment" minOccurs="0"/>
            <xs:element ref="nfdem:DateTimeSampleCollected" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:ItemFoundLocation" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:PackagingMethod" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:RecoveredBy" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="CrimeSceneEntryLog" type="nfdem:CrimeSceneEntryLogType">
        <xs:annotation>
            <xs:documentation> CrimeSceneEntryLog document corresponds to the Crime Scene Entry Log
                for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="CrimeSceneEntryLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for CrimeSceneEntryLog element which corresponds to
