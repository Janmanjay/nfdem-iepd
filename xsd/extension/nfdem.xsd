<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
    ct:conformanceTargets="http://reference.niem.gov/niem/specification/naming-and-design-rules/4.0/#ExtensionSchemaDocument"
    targetNamespace="http://www.dtechspace.com/xml-schema/nfdem/0.5/" version="0.5"
    xmlns:nfdem="http://www.dtechspace.com/xml-schema/nfdem/0.5/"
    xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/"
    xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/"
    xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:cbrn="http://release.niem.gov/niem/domains/cbrn/4.0/"
    xmlns:jxdm="http://release.niem.gov/niem/domains/jxdm/6.0/"
    xmlns:mmucc="http://release.niem.gov/niem/codes/mmucc/4.0/"
    xmlns:unece="http://release.niem.gov/niem/codes/unece_rec20/4.0/"
    xmlns:biom="http://release.niem.gov/niem/domains/biometrics/4.0/"
    xmlns:hs="http://release.niem.gov/niem/domains/humanServices/4.0/"
    elementFormDefault="qualified">

    <xs:annotation>
        <xs:documentation> This extension schema defines the elements and types for the Nuclear Forensics Data Exchange Model (NFDEM). </xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../niem/niem-core/4.0/niem-core.xsd" namespace="http://release.niem.gov/niem/niem-core/4.0/"/>
    <xs:import schemaLocation="../niem/proxy/xsd/4.0/xs.xsd" namespace="http://release.niem.gov/niem/proxy/xsd/4.0/"/>
    <xs:import schemaLocation="../niem/domains/cbrn/4.0/cbrn.xsd" namespace="http://release.niem.gov/niem/domains/cbrn/4.0/"/>
    <xs:import schemaLocation="../niem/domains/jxdm/6.0/jxdm.xsd" namespace="http://release.niem.gov/niem/domains/jxdm/6.0/"/>
    <xs:import schemaLocation="../niem/codes/mmucc/4.0/mmucc.xsd" namespace="http://release.niem.gov/niem/codes/mmucc/4.0/"/>
    <xs:import schemaLocation="../niem/codes/unece_rec20/4.0/unece_rec20-misc.xsd" namespace="http://release.niem.gov/niem/codes/unece_rec20/4.0/"/>
    <xs:import schemaLocation="../niem/domains/biometrics/4.0/biom.xsd" namespace="http://release.niem.gov/niem/domains/biometrics/4.0/"/>
    <xs:import schemaLocation="../niem/domains/humanServices/4.0/hs.xsd" namespace="http://release.niem.gov/niem/domains/humanServices/4.0/"/>
    <!--xs:import schemaLocation="../niem/utility/structures/4.0/structures.xsd" namespace="http://release.niem.gov/niem/structures/4.0/"/-->

    <xs:element name="CaseList" type="nfdem:Case">
        <xs:annotation>
            <xs:documentation> CaseList document corresponds to the Case Identifier Form for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:complexType name="Case">
        <xs:annotation>
            <xs:documentation> XML Schema type for Case type which consists of the unique Case Identifier and details of CaseList for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:IncidentManager" minOccurs="1"/>
            <xs:element ref="nfdem:DateTimeCreated" minOccurs="1"/>
            <xs:element ref="nfdem:DateTimeUpdated" minOccurs="1"/>
            <xs:element ref="nfdem:CaseStatus" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="1"/>
            <xs:element ref="nfdem:Comment" minOccurs="1"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>

        </xs:sequence>
    </xs:complexType>

    <xs:element name="AdminWorksheet" type="nfdem:AdminWorksheetType">
        <xs:annotation>
            <xs:documentation> AdminWorksheet document corresponds to the Administrative Worksheet
                form for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="AdminWorksheetType">
        <xs:annotation>
            <xs:documentation> XML Schema type for AdminWorksheet element which corresponds to the
                Administrative Worksheet form for crime scene investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation"/>
            <xs:element ref="nfdem:ArrivalDateTime" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:LightingCondition" minOccurs="1"/>
            <xs:element ref="nfdem:PersonPresentOnArrival" minOccurs="0"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1"/>
            <xs:element ref="nfdem:AssistantPreparer" minOccurs="1"/>
            <xs:element ref="nfdem:WeatherCondition" minOccurs="1"/>
            <xs:element name="SceneConsistsOf" type="nc:TextType"/>
            <xs:element name="PersonnelInvolvedAndTheirDuties" type="nc:TextType"/>
            <xs:element name="PreliminarySurveyEvaluation" type="nc:TextType"/>
            <xs:element name="SpecialConditions" type="nc:TextType"/>
            <xs:element name="CrimeSceneConference" type="nc:TextType"/>
            <xs:element name="FinalSurvey" type="nc:TextType"/>
            <xs:element ref="nfdem:CrimeSceneReleasedByList" minOccurs="1"/>
            <xs:element ref="nfdem:CrimeSceneReleasedToList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="RadiationSurveyInfo" type="nfdem:RadiationSurveyInfoType">
        <xs:annotation>
            <xs:documentation> RadiationSurveyInfo document corresponds to the Radiation Survey
                Information form for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="RadiationSurveyInfoType">
        <xs:annotation>
            <xs:documentation> XML Schema type for RadiationSurveyInfo element which corresponds to
                the Radiation Survey Information form for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:InstrumentCalibrationDateTime" minOccurs="1"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1" maxOccurs="5"/>
            <xs:element ref="cbrn:RadInstrumentInformation" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="cbrn:RadInstrumentModelName" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SequenceNumber" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:Signature" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SurveyLocationMapInformation" minOccurs="1" maxOccurs="unbounded"
            />
        </xs:sequence>
    </xs:complexType>

    <xs:element name="PersonalRadMonitoringLog" type="nfdem:PersonalRadMonitoringLogType">
        <xs:annotation>
            <xs:documentation> PersonalRadMonitoringLog document corresponds to the Personal
                External Radiation Monitoring Log for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="PersonalRadMonitoringLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for PersonalRadMonitoringLog element which
                corresponds to the Personal External Radiation Monitoring Log for crime scene
                investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:Comment" minOccurs="0"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:DosimeterType" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:PersonMonitored" minOccurs="0"/>
            <xs:element ref="nfdem:PersonMonitoredIdentification" minOccurs="0"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="cbrn:RadInstrumentInformation" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="cbrn:RadInstrumentModelName" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:ReadingDateTime" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:ReadingTimeDuration" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:SerialNumber" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="SceneInfo" type="nfdem:SceneInfoType">
        <xs:annotation>
            <xs:documentation> SceneInfo document corresponds to the Scence Information form for
                crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="SceneInfoType">
        <xs:annotation>
            <xs:documentation> XML Schema type for SceneInfo element which corresponds to the Scence
                Information form for crime scene investigatio. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:ArrivalDateTime" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element ref="nfdem:PreparedBy" minOccurs="1" maxOccurs="5"/>
            <xs:element ref="nfdem:SceneReferenceInformation" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="ChainofCustodyGeneral" type="nfdem:ChainofCustodyGeneralType">
        <xs:annotation>
            <xs:documentation> ChainofCustodyGeneral document corresponds to the generic Chain of
                Custody form for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="ChainofCustodyGeneralType">
        <xs:annotation>
            <xs:documentation> XML Schema type for ChainofCustodyGeneral element which corresponds
                to the generic Chain of Custody form for crime scene investigation.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:SampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="ChainofCustodyEnvironmental" type="nfdem:ChainofCustodyEnvironmentalType">
        <xs:annotation>
            <xs:documentation> ChainofCustodyEnvironmentalType document corresponds to the Chain of
                Custody form for Environmental Analysis. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="ChainofCustodyEnvironmentalType">
        <xs:annotation>
            <xs:documentation> XML Schema type for ChainofCustodyEnvironmental element which
                corresponds to the Chain of Custody form for Environmental Analysis.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:EnvironmentalSampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="ChainofCustodyGeoChemical" type="nfdem:ChainofCustodyGeoChemicalType">
        <xs:annotation>
            <xs:documentation> ChainofCustodyGeoChemicalType document corresponds to the Chain of
                Custody form for GeoChemical Analysis. </xs:documentation>
        </xs:annotation>
    </xs:element>
    
    <xs:complexType name="ChainofCustodyGeoChemicalType">
        <xs:annotation>
            <xs:documentation> XML Schema type for ChainofCustodyGeoChemicalType element which
                corresponds to the Chain of Custody form for GeoChemical Analysis.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:GeoChemicalSampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="ChainofCustodyRadiological" type="nfdem:ChainofCustodyRadiologicalType">
        <xs:annotation>
            <xs:documentation> ChainofCustodyRadiologicalType document corresponds to the Chain of
                Custody form for Radiological Analysis. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="ChainofCustodyRadiologicalType">
        <xs:annotation>
            <xs:documentation> XML Schema type for ChainofCustodyGeneral element which corresponds
                to the Chain of Custody form for Radiological Analysis. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element ref="nfdem:GeoLocation" minOccurs="1"/>
            <xs:element ref="nfdem:ObtainedFrom" minOccurs="1"/>
            <xs:element ref="nfdem:Description" minOccurs="0"/>
            <xs:element ref="nfdem:ContactInformation" minOccurs="1"/>
            <xs:element ref="nfdem:BillingInformation" minOccurs="1"/>
            <xs:element ref="nfdem:ProjectName" minOccurs="1"/>
            <xs:element ref="nfdem:EvidenceIdentificationNumber" minOccurs="0"/>
            <xs:element ref="nfdem:TurnAroundTime" minOccurs="1"/>
            <xs:element ref="nfdem:SampleCollectedBy" minOccurs="1"/>
            <xs:element ref="nfdem:Signature" minOccurs="1"/>
            <xs:element ref="nfdem:SampleQuantity" minOccurs="1"/>
            <xs:element ref="nfdem:ItemStorageLocation" minOccurs="1"/>
            <xs:element ref="cbrn:DoseRateValue" minOccurs="1"/>
            <xs:element ref="nfdem:RadiologicalSampleList" minOccurs="1"/>
            <xs:element ref="nfdem:CustodyList" minOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="PhotographicLog" type="nfdem:PhotographicLogType">
        <xs:annotation>
            <xs:documentation> PhotographicLog document corresponds to the Photographic Log for
                crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="PhotographicLogType">
        <xs:annotation>
            <xs:documentation> XML Schema type for PhotographicLog element which corresponds to the
                Photographic Log for crime scene investigation. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="nfdem:CaseIdentifier" minOccurs="1"/>
            <xs:element name="Date" minOccurs="1" maxOccurs="unbounded" type="niem-xs:date">
                <xs:annotation>
                    <xs:documentation> XML Schema type for Date Element. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="nfdem:Description" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="Photographer" minOccurs="1" maxOccurs="unbounded" type="nc:PersonType">
                <xs:annotation>
                    <xs:documentation> XML Schema type for Photographer Element. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PhotoIdentificationNumber" minOccurs="1" maxOccurs="unbounded"
                type="nc:IdentificationType">
                <xs:annotation>
                    <xs:documentation> XML Schema type for PhotoIdentificationNumber Element.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="EvidenceRecoveryLogCover" type="nfdem:EvidenceRecoveryLogCoverType">
        <xs:annotation>
            <xs:documentation> EvidenceRecoveryLogCover document corresponds to the Evidence
                Recovery Log Cover Sheet for crime scene investigation. </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="EvidenceRecoveryLogCoverType">
        <xs:annotation>
            <xs:documentation> XML Schema type for EvidenceRecoveryLogCover element which
